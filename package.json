{
    "name": "rails-project-navigator",
    "displayName": "Rails Project Navigator",
    "description": "Easily navigate the files in Rails applications",
    "version": "0.0.2",
    "publisher": "brandoncc",
    "repository": "https://github.com/brandoncc/rails-project-navigator",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.navigateRailsProject"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.navigateRailsProject",
            "title": "Navigate Rails Project"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
